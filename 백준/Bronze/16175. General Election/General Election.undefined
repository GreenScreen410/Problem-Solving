#include <bits/stdc++.h>
using namespace std;

int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(nullptr);

  int T;
  cin >> T;

  while (T--) {
    int N, M;
    cin >> N >> M;

    vector<int> v(N);
    for (int i = 0; i < M; i++) {
      for (int j = 0; j < N; j++) {
        int vote;
        cin >> vote;
        v[j] += vote;
      }
    }

    cout << (distance(v.begin(), max_element(v.begin(), v.end())) + 1) << "\n";
  }

  return 0;
}